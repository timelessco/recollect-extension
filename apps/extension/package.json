{
  "name": "recollect-extension",
  "version": "0.2.0",
  "private": true,
  "description": "Recollect browser extension",
  "type": "module",
  "scripts": {
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "check": "pnpm dlx ultracite check --type-aware --type-check",
    "compile": "tsc --noEmit",
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "fix": "pnpm dlx ultracite fix --type-aware --type-check",
    "knip": "pnpm dlx knip",
    "postinstall": "wxt prepare",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox"
  },
  "dependencies": {
    "@fontsource-variable/inter": "5.2.8",
    "@react-hookz/web": "25.2.0",
    "@repo/shadcn-ui": "workspace:*",
    "@tailwindcss/vite": "4.1.18",
    "lucide-react": "0.563.0",
    "react": "catalog:",
    "react-dom": "catalog:",
    "tailwindcss": "4.1.18",
    "tw-animate-css": "1.4.0"
  },
  "devDependencies": {
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "25.0.10",
    "@types/react": "catalog:",
    "@types/react-dom": "catalog:",
    "@wxt-dev/module-react": "1.1.5",
    "shadcn": "3.7.0",
    "typescript": "5.9.3",
    "wxt": "0.20.13"
  }
}
